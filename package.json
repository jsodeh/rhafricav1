  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:staging": "VITE_BUILD_TARGET=staging vite build",
    "build:production": "VITE_BUILD_TARGET=production vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:production": "npm run build:production && vite preview",
    "deploy:check": "node scripts/deploy-config.js",
    "deploy:staging": "npm run deploy:check && npm run build:staging",
    "deploy:production": "npm run deploy:check && npm run build:production",
    "analyze": "npx vite-bundle-analyzer",
    "lighthouse": "npx lighthouse http://localhost:4173 --output=html --output-path=./lighthouse-report.html",
    "performance:test": "npm run build && npm run preview & sleep 5 && npm run lighthouse && kill %1",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:ci": "npm run test:run && npm run test:e2e -- --reporter=dot"
