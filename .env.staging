# Staging Environment Configuration for Real Estate Hotspot

# Supabase Configuration (Staging)
VITE_SUPABASE_URL=https://your-staging-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_staging_supabase_anon_key

# Payment Integration (Test Mode)
VITE_PAYSTACK_PUBLIC_KEY=pk_test_your_staging_paystack_public_key

# Mapbox Access Token (Staging)
VITE_MAPBOX_ACCESS_TOKEN=your_staging_mapbox_access_token

# App Configuration (Staging)
VITE_APP_NAME=Real Estate Hotspot (Staging)
VITE_APP_URL=https://staging.realestatehotspot.com
VITE_API_URL=https://staging-api.realestatehotspot.com

# Analytics & Monitoring (Test)
VITE_GA_MEASUREMENT_ID=G-STAGING-TEST
VITE_HOTJAR_ID=your_staging_hotjar_id
VITE_SENTRY_DSN=your_staging_sentry_dsn

# Features Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_PWA=false

# Security (Relaxed for testing)
VITE_ENABLE_CSP=false
VITE_ENABLE_RATE_LIMITING=false

# Build Configuration
NODE_ENV=production
VITE_BUILD_TARGET=staging
